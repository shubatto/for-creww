<!DOCTYPE html>
<html>
  <head>
		<title><%= full_title(yield(:title)) %></title>
		<%= render 'layouts/rails_default' %>
		<%= render 'layouts/shim' %>
  </head>
  <body>
		<div class="container">
			<% flash.each do |message_type, message| %>
				<%= content_tag(:div, message, class: "alert alert-#{message_type}") %>
			<% end %>
			<h1>お問い合わせ</h1>
    	<%= yield %>
			<%= render 'layouts/footer' %>
		</div>
  </body>
</html>
